{
    "name": "phpmob/changmin",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": "^7.2",
        "danielstjules/stringy": "^3.1",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "egeloen/ckeditor-bundle": "^6.0",
        "friendsofsymfony/rest-bundle": "^2.1",
        "fzaninotto/faker": "^1.6",
        "helios-ag/fm-elfinder-bundle": "^6.2",
        "hwi/oauth-bundle": "^0.6.0",
        "incenteev/composer-parameter-handler": "^2.0",
        "league/color-extractor": "0.3.*",
        "league/flysystem-cached-adapter": "^1.0",
        "liip/imagine-bundle": "^1.6 || ^2.0",
        "matthiasmullie/minify": "^1.3",
        "ocramius/proxy-manager": "^2.1",
        "oneup/flysystem-bundle": "^2.0 || ^3.0",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/httplug-bundle": "^1.8",
        "phpmob/settings-bundle": "^1.0",
        "phpmob/twig-modify-bundle": "^1.0",
        "rollerworks/password-strength-validator": "^1.0",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^3.0.2",
        "sonata-project/intl-bundle": "^2.2",
        "sylius/fixtures-bundle": "^1.1",
        "sylius/registry": "^1.1",
        "sylius/grid": "^1.1",
        "sylius/grid-bundle": "^1.1",
        "sylius/locale": "^1.1",
        "sylius/locale-bundle": "^1.1",
        "sylius/user": "^1.1",
        "sylius/user-bundle": "^1.1",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^2.3.10",
        "symfony/symfony": "3.4.*",
        "symfony/twig-bundle": "^3.3",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "sylius/taxonomy-bundle": "^1.1",
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Ivory\\CKEditorBundle\\Composer\\CKEditorScriptHandler::install",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin",
        "component-dir": "web/assets"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "ckeditor-clear": "drop",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
