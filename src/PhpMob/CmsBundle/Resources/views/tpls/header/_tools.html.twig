{% if not is_route_name('web_security_register') %}
    <a class="btn btn-info btn-sm mr-2" href="{{ path('web_security_register') }}">
        <i class="fa fa-user-plus"></i> {{ "Register" }}
    </a>
{% endif %}

{% if not is_route_name('web_security_login') %}
    <a class="btn btn-info btn-sm" href="{{ path('web_security_login') }}">
        <i class="fa fa-sign-in"></i> {{ "Login" }}
    </a>
{% endif %}
