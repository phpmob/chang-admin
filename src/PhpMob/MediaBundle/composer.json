{
    "name": "phpmob/media-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "PhpMob\\MediaBundle": ""
        }
    },
    "require": {
        "php": "^7.1",

        "doctrine/doctrine-cache-bundle": "^1.3",
        "doctrine/phpcr-bundle": "^1.3",
        "helios-ag/fm-elfinder-bundle": "^6.2",
        "jackalope/jackalope-doctrine-dbal": "^1.3",
        "league/flysystem-cached-adapter": "^1.0",
        "league/flysystem-phpcr": "dev-master",
        "liip/imagine-bundle": "^1.6",
        "oneup/flysystem-bundle": "^2.0",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "phpspec/phpspec": "^4.0"
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
